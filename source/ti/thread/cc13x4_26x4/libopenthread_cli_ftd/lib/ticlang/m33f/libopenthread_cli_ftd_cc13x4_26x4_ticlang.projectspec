<?xml version="1.0" encoding="UTF-8"?>
<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="com.ti.ccstudio.deviceModel.TMS470.CustomTMS470Device"
            />
