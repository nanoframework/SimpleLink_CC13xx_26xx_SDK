        <file path="${COM_TI_SIMPLELINK_CC13XX_CC26XX_SDK_INSTALL_DIR}/source/third_party/openthread/src/cli/cli_config.h" openOnCreation="false" excludeFromBuild="false" action="link" targetDirectory="src/cli">
